pico-8 cartridge // http://www.pico-8.com
version 4
__lua__
-- INIT
function createplayer()
	local pl = {}

	pl.x = 59
	pl.y = 100
	pl.score = 0
	pl.highscore = 0
	pl.spriteid = 1
	pl.lives = 3
	pl.speed = 2
	pl.shooting = false
	pl.bulletspeed = -4

	return pl
end

function gamerules()
	local ru = {}

	ru.boundxmin = 2
	ru.boundxmax = 118
	ru.boundymin = 10
	ru.boundymax = 116
	ru.enemiesincolumn = 4

	return ru
end

function initbullets()
	local bls = {}
	return bls
end

function createbullet(x, y, speed)
	local bu = {}

	bu.x = x
	bu.y = y
	bu.speed = speed
	bu.spriteid = 7

	return bu
end

function initenemies()
	local en = {}
	return en
end

function createenemy(x, y, spriteid, column, score)
	local en = {}

	en.x = x
	en.y = y
	en.spriteid = spriteid
	en.column = column
	en.score = score

	return en
end

function createcolumnofenemies(column, number)
	x = 18 + (14 * column)
	y = 25
	e = 0
	score = 30
	for i = 1, number do
		add(enemies, createenemy(x, y, (2 + e), column, score))
		y += 10
		if (i != 2) then
			e = e+1
			score = score - 10
		end
	end
end

function _init()
	player = createplayer()
	bullets = initbullets()
	enemies = initenemies()
	rules = gamerules()

	for i = 0, 6 do
		createcolumnofenemies(i, rules.enemiesincolumn)
	end
end

-- UPDATE
function _update()
	handleinput()
	checkbounds()
	updatebullets()
	checkcollision()

	if (player.highscore < player.score) then
		player.highscore = player.score
	end
end

function handleinput()
	-- move left
	if(btn(0)) then
		player.x -= player.speed
	end
	-- move right
	if(btn(1)) then
		player.x += player.speed
	end

	if(btn(4)) then
		if(player.shooting == false) then
			playershoot()
		end
	end
end

function playershoot()
	add(bullets, createbullet(player.x, player.y, player.bulletspeed))
	player.shooting = true
end

function updatebullets() 
	for bullet in all(bullets) do
		bullet.y += bullet.speed
	end
end

function checkbounds()
	if (player.x < rules.boundxmin) then
		player.x = rules.boundxmin
	end
	if (player.x > rules.boundxmax) then
		player.x = rules.boundxmax
	end

	for bullet in all(bullets) do
		if (bullet.y < rules.boundymin) then
			player.shooting = false
			del(bullets, bullet)
		end
		if (bullet.y > rules.boundymax) then
			del(bullets, bullet)
		end
	end
end

function checkcollision()
	for bullet in all(bullets) do
		for enemy in all(enemies) do
			if ((bullet.x < (enemy.x+8) and (bullet.x+8) > enemy.x))
				and (bullet.y < (enemy.y+8) and (bullet.y+8) > enemy.y) then
				player.score += enemy.score
				del(bullets, bullet)
				del(enemies, enemy)
				player.shooting = false
			end
		end
	end
end

-- DRAW
function _draw()
	cls()
	rectfill(0, 0, 127, 127, 0)
	drawui()
	spr(player.spriteid, player.x, player.y)
	drawbullets()
	drawenemies()
end

function drawui()
	-- lives
	x = 2
	for i = 1, player.lives do
		spr(player.spriteid, x, 118)
		x = x + 10
	end
	
	line(0, 116, 127, 116, 3)

	-- score
	print("score", 2, 2, 7)
	print(player.score, 24, 2, 7)
	-- highscore 112
	print("highscore", 74, 2, 7)
	print(player.highscore, 112, 2, 7)
end

function drawbullets()
	for bullet in all(bullets) do
		spr(bullet.spriteid, bullet.x, bullet.y)
	end
end

function drawenemies()
	for enemy in all(enemies) do
		spr(enemy.spriteid, enemy.x, enemy.y)
	end
end
